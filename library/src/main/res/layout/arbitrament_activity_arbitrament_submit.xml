<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/view_statusbar_placeholder"
        android:layout_width="match_parent"
        android:layout_height="@dimen/uikit_status_bar_height" />

    <WebView
        android:id="@+id/wv_pdf"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <View style="@style/UikitDivider.Horizontal" />

    <com.hm.iou.uikit.HMBottomBarView
        android:id="@+id/bottomBar"
        android:layout_width="match_parent"
        android:layout_height="50dip"
        app:bottomTitleIcon="@mipmap/uikit_ic_more_black"
        app:bottomTitleIconWidth="50dp"
        app:bottomTitleText="提交仲裁" />

</LinearLayout>
